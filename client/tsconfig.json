{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "jsx": "react",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "types": ["node", "react", "@mui/material"],
    "allowImportingTsExtensions": true
  },
  "exclude": ["node_modules"]
}